    <style>
       .search-container {
        text-align: center;
        }

        .search-wrapper {
        display: inline-block;
        position: relative;
        }

        .search-bar {
        padding: 10px;
        font-size: 16px;
        border: 1px solid #1672e9;
        border-radius: 4px;
        width: 1000px;
        }

        .search-button {
        position: absolute;
        top: 0;
        right: 0;
        padding: 10px;
        font-size: 16px;
        border: none;
        background-color: #037b0d;
        color: #fff;
        cursor: pointer;
        border-radius: 0 4px 4px 0;
